	4. 0xfffffefb

6)	1. -5
	2. -22
	3. -13
	4. -8

7)	1. no
	2. no
	3. no
	4. no
	5. yes

8)	1. 0b 1100 0001 0100 0100 0000 0000 0000 0000
	   0b 1 10000010 10001000000000000000000
	   = -2^(3) * 1.10001 (base 2)
	   = -8 * 1.53125
	   = -12.25
	2. 0b 0100 0001 0100 0100 0000 0000 0000 0000
	   0b 0 10000010 10001000000000000000000
	   = 2^(3) * 1.10001 (base 2)
	   = 8 * 1.53125
	   = 12.25
	3. 0b 1100 0000 1101 0000 0000 0000 0000 0000
	   0b 1 10000001 10100000000000000000000
	   = -2^(2) * 1.101 (base 2)
	   = -4 * 1.625
	   = -6.5
	4. 0b 1100 0000 1111 0000 0000 0000 0000 0000
	   0b 1 10000001 11100000000000000000000
	   = -2^(2) * 1.111 (base 2)
	   = -4 * 1.875
	   = -7.5

9)	1. 11.25 (base 10) 
	   = 1011.01 (base 2)
	   = 2^(3) * 1.01101
	   0b 0 10000010 01101000000000000000000
	   0b 0100 0001 0011 0100 0000 0000 0000 0000
	   0x 41340000
	2. -17.125 (base 10)
	   = -10001.001 (base 2)
	   = -2^(4) * 1.0001001
	   0b 1 10000011 00010010000000000000000
	   0b 1100 0001 1000 1001 0000 0000 0000 0000
	   0x c1890000
	3. 21.875 (base 10)
	   = 10101.111 (base 2)
	   = 2^(4) * 1.0101111
	   0b 0 10000011 01011110000000000000000
	   0b 0100 0001 1010 1111 0000 0000 0000 0000
	   0x 41af0000
	4. -0.75 (base 10)
	   = -0.11 (base 2)
	   = -2^(-1) * 1.1
	   0b 1 01111110 10000000000000000000000
	   0b 1011 1111 0100 0000 0000 0000 0000 0000
	   0x bf400000

